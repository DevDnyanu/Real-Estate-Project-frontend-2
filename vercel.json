{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://real-estate-project-backend-2-2.onrender.com/api/$1"
    },
    {
      "source": "/profile",
      "destination": "/dist/index.html"
    },
    {
      "source": "/listing/:id*",
      "destination": "/dist/index.html"
    },
    {
      "source": "/properties",
      "destination": "/dist/index.html"
    },
    {
      "source": "/listings",
      "destination": "/dist/index.html"
    },
    {
      "source": "/create-listing",
      "destination": "/dist/index.html"
    },
    {
      "source": "/edit/:id*",
      "destination": "/dist/index.html"
    },
    {
      "source": "/packages",
      "destination": "/dist/index.html"
    },
    {
      "source": "/login",
      "destination": "/dist/index.html"
    },
    {
      "source": "/signup",
      "destination": "/dist/index.html"
    },
    {
      "source": "/forgot-password",
      "destination": "/dist/index.html"
    },
    {
      "source": "/verify-otp",
      "destination": "/dist/index.html"
    },
    {
      "source": "/reset-password",
      "destination": "/dist/index.html"
    },
    {
      "source": "/about",
      "destination": "/dist/index.html"
    },
    {
      "source": "/contact",
      "destination": "/dist/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/index.html"
    }
  ]
}