{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/profile",
      "dest": "/dist/index.html"
    },
    {
      "src": "/listing/.*",
      "dest": "/dist/index.html"
    },
    {
      "src": "/properties",
      "dest": "/dist/index.html"
    },
    {
      "src": "/listings",
      "dest": "/dist/index.html"
    },
    {
      "src": "/create-listing",
      "dest": "/dist/index.html"
    },
    {
      "src": "/edit/.*",
      "dest": "/dist/index.html"
    },
    {
      "src": "/packages",
      "dest": "/dist/index.html"
    },
    {
      "src": "/login",
      "dest": "/dist/index.html"
    },
    {
      "src": "/signup",
      "dest": "/dist/index.html"
    },
    {
      "src": "/forgot-password",
      "dest": "/dist/index.html"
    },
    {
      "src": "/verify-otp",
      "dest": "/dist/index.html"
    },
    {
      "src": "/reset-password",
      "dest": "/dist/index.html"
    },
    {
      "src": "/about",
      "dest": "/dist/index.html"
    },
    {
      "src": "/contact",
      "dest": "/dist/index.html"
    }
  ]
}